<mvc:View controllerName="s.zca11lastex.controller.View1"                                                                                                                                                                                                      
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"                                                                                                                                                                                                            
    xmlns="sap.m"                                                                                                                                                                                                                                              
    xmlns:core="sap.ui.core"                                                                                                                                                                                                                                   
    >                                                                                                                                                                                                                                                          
    <Page id="page" title="{i18n>title}">                                                                                                                                                                                                                      
        <content>                                                                                                                                                                                                                                              
        <Label text="선택년도:" />                                                                                                                                                                                                                                 
            <ComboBox id="idCb" items="{/ES_SYEARSet}">                                                                                                                                                                                                        
                <items>                                                                                                                                                                                                                                        
                    <core:Item key="{Syear}" text="{Syeart}" />                                                                                                                                                                                                
                </items>                                                                                                                                                                                                                                       
            </ComboBox>                                                                                                                                                                                                                                        
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                       
            <Button icon="sap-icon://search" press="onClick" ></Button>                                                                                                                                                                                        
<Panel expand="true" expandable="true" headerText="항공사별 매출액" >                                                                                                                                                                                                 
            <Table id="idSalesTab" items="{/ES_SALESSet}" >                                                                                                                                                                                                    
                <columns>                                                                                                                                                                                                                                      
                    <Column><header><Text text="년도" /></header></Column>                                                                                                                                                                                       
                    <Column><header><Text text="항공사" /></header></Column>                                                                                                                                                                                      
                    <Column><header><Text text="항공사명" /></header></Column>                                                                                                                                                                                     
                    <Column><header><Text text="예약 건수" /></header></Column>                                                                                                                                                                                    
                    <Column><header><Text text="매출액" /></header></Column>                                                                                                                                                                                      
                    <Column><header><Text text="취소 건수" /></header></Column>                                                                                                                                                                                    
                </columns>                                                                                                                                                                                                                                     
                <items>                                                                                                                                                                                                                                        
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                       
                    <ColumnListItem id="idCli" press="onPress" type="Active">                                                                                                                                                                                  
                        <cells>                                                                                                                                                                                                                                
                            <Text text="{Syear}" />                                                                                                                                                                                                            
                            <Text text="{Carrid}" />                                                                                                                                                                                                           
                            <Text text="{Carrname}" />                                                                                                                                                                                                         
                            <Text text="{Bkcnt}" />                                                                                                                                                                                                            
                            <Text text="{Curam}" />                                                                                                                                                                                                            
                            <Text text="{Cacnt}" />                                                                                                                                                                                                            
                        </cells>                                                                                                                                                                                                                               
                    </ColumnListItem>                                                                                                                                                                                                                          
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                               
                </items>                                                                                                                                                                                                                                       
            </Table>                                                                                                                                                                                                                                           
            </Panel>                                                                                                                                                                                                                                           
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;                                                                                                                                                                                       
        </content>                                                                                                                                                                                                                                             
    </Page>                                                                                                                                                                                                                                                    
</mvc:View>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               