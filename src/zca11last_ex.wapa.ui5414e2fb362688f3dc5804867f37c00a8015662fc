sap.ui.define([                                                                                                                                                                                                                                                
    "sap/ui/core/mvc/Controller",                                                                                                                                                                                                                              
    "sap/ui/model/Filter"                                                                                                                                                                                                                                      
],                                                                                                                                                                                                                                                             
    /**                                                                                                                                                                                                                                                        
     * @param {typeof sap.ui.core.mvc.Controller} Controller                                                                                                                                                                                                   
     */                                                                                                                                                                                                                                                        
    function (Controller, Filter) {                                                                                                                                                                                                                            
        "use strict";                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
        return Controller.extend("s.zca11lastex.controller.View1", {                                                                                                                                                                                           
            onInit: function () {                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            onClick: function(){                                                                                                                                                                                                                               
                let selectkey = this.getView().byId("idCb").getSelectedKey();                                                                                                                                                                                  
                let oSalesTab = this.getView().byId("idSalesTab");                                                                                                                                                                                             
                let oFilter = new Filter(                                                                                                                                                                                                                      
                    "Syear", "EQ", selectkey                                                                                                                                                                                                                   
                    );                                                                                                                                                                                                                                         
                oSalesTab.getBinding("items").filter(oFilter);                                                                                                                                                                                                 
            },                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
            onPress: function ( oEvt ){                                                                                                                                                                                                                        
                let sPath = oEvt.getSource().getBindingContext().getPath();                                                                                                                                                                                    
                let oCli = this.getView().byId("idCli");                                                                                                                                                                                                       
                oCli.bindElement( sPath );                                                                                                                                                                                                                     
            }                                                                                                                                                                                                                                                  
        });                                                                                                                                                                                                                                                    
    });                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               